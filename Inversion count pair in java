Inversion count pair
import java.util.*;
class Test{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int i,j;
        int n=s.nextInt();
        int a[]=new int[n];
        for(i=0;i<n;i++){
            a[i]=s.nextInt();
        }
       int c=0;
       j=0;
       for(i=0;i<n;i++){
           for(j=i+1;j<n;j++){
              if(a[i]>a[j])
                  c++;
       }
       }
       System.out.println(c);
    }
}
7
13 10 9 6 21 15 14
9
